{
  "name": "raga-detect-native",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "build:ios": "cd ios && pod install && cd .. && react-native run-ios --configuration Release",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "test": "jest"
  },
  "dependencies": {
    "@lynx-js/core": "^1.0.0",
    "@lynx-js/audio": "^1.0.0",
    "@lynx-js/native": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-fs": "^2.20.0",
    "react-native-permissions": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/runtime": "^7.22.0",
    "@react-native-community/cli": "^11.3.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.76.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|@lynx-js)/)"
    ]
  },
  "reactNative": {
    "projectRoot": ".",
    "assets": ["./assets/fonts/"]
  }
}
